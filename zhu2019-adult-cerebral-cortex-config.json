{
    "experiment-name": "zhu-2019-adult-cerebral-cortex-integration",
    "dataset": {
        "datasets": [
            {
                "name": "zhu2019-adult-cerebrail-cortex-scrna",
                "root-dir": "data/zhu-2019/Adult_Cerebrail_Cortex/Adult_CTX_RNA",
                "h5ad-filename": "expressions.h5ad"
            },
            {
                "name": "zhu2019-adult-cerebrail-cortex-scatac",
                "root-dir": "data/zhu-2019/Adult_Cerebrail_Cortex/Adult_CTX_DNA",
                "h5ad-filename": "peaks-binary.h5ad"
            }
        ],
        "batch_size": 64,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscvae",
        "params": {
            "paired": true,
            "recon_coef": 1,
            "kl_coef": 0.0001,
            "cross_coef": 0,
            "x_dims": [2693, 1002],
            "z_dim": 10,
            "hiddens": [256, 128, 64, 32]
        }
    },
    "train": {
        "n-epochs": 50,
        "optimizer": {
            "name": "adam",
            "lr": 0.0001
        },
        "output-dir": "outputs/",
        "seed": 42
    }
}