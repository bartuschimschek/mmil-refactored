{
    "experiment-name": "granja-2019-normal-cells-integration",
    "dataset": {
        "datasets": [
            {
                "name": "granja2019-scrna",
                "root-dir": "data/granja-2019/scRNA/",
                "h5ad-filename": "expressions-ctrl.h5ad"
            },
            {
                "name": "zhu2019-adult-cerebrail-cortex-scatac",
                "root-dir": "data/granja-2019/scATAC/peaks/",
                "h5ad-filename": "peaks-binary-ctrl.h5ad"
            }
        ],
        "batch_size": 64,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscvae",
        "params": {
            "paired": false,
            "recon_coef": 1,
            "kl_coef": 0.0001,
            "cross_coef": 1,
            "x_dims": [2999, 20002],
            "z_dim": 20,
            "hiddens": [256, 128, 64, 32]
        }
    },
    "train": {
        "n-epochs": 50,
        "optimizer": {
            "name": "adam",
            "lr": 0.0001
        },
        "output-dir": "outputs/",
        "seed": 42
    }
}