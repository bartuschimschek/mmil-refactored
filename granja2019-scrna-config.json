{
    "experiment-name": "granja-2019-normal-cells-scrna",
    "dataset": {
        "datasets": [
            {
                "name": "granja2019-scrna",
                "root-dir": "data/granja-2019/scRNA/",
                "h5ad-filename": "expressions-ctrl.h5ad"
            }
        ],
        "batch_size": 64,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscvae",
        "params": {
            "paired": false,
            "recon_coef": 1,
            "kl_coef": 0.000001,
            "cross_coef": 0,
            "x_dims": [2999],
            "z_dim": 20,
            "hiddens": [256, 128, 64, 32]
        }
    },
    "train": {
        "n-epochs": 50,
        "optimizer": {
            "name": "adam",
            "lr": 0.0001
        },
        "output-dir": "outputs/",
        "seed": 42
    }
}