{
    "experiment-name": "chen-2019-ad-brain-cortex-integration",
    "dataset": {
        "datasets": [
            {
                "name": "chen2019-ad-brain-cortex-scrna",
                "root-dir": "data/chen-2019/AdBrainCortex/rna",
                "h5ad-filename": "expressions.h5ad"
            },
            {
                "name": "chen2019-ad-brain-cortex-scatac",
                "root-dir": "data/chen-2019/AdBrainCortex/atac",
                "h5ad-filename": "peaks-binary.h5ad"
            }
        ],
        "batch_size": 64,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscvae",
        "params": {
            "paired": true,
            "recon_coef": 1,
            "kl_coef": 0.00001,
            "cross_coef": 1,
            "x_dims": [2967, 30104],
            "z_dim": 20,
            "hiddens": [256, 128, 64, 32]
        }
    },
    "train": {
        "n-epochs": 1000,
        "optimizer": {
            "name": "adam",
            "lr": 0.001
        },
        "early-stopping": 200,
        "output-dir": "outputs/",
        "seed": 42
    }
}