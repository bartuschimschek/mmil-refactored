{
    "dataset": {
        "datasets": [
            {
                "name": "scrna",
                "root-dir": "data/chen-2019/P0BrainCortex/rna",
                "h5ad-filename": "expressions.h5ad"
            },
            {
                "name": "scatac",
                "root-dir": "data/chen-2019/P0BrainCortex/atac",
                "h5ad-filename": "peaks-binary.h5ad"
            }
        ],
        "batch_size": 64,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscae",
        "params": {
            "paired": true,
            "recon_coef": 1,
            "cross_coef": 0,
            "integ_coef": 0,
            "cycle_coef": 0,
            "x_dims": [4308, 30298],
            "z_dim": 20,
            "h_dim": 256,
            "hiddens": [300],
            "shared_hiddens": [128, 64],
            "dropout": 0.2
        }
    },
    "train": {
        "n-epochs": 25000,
        "optimizer": {
            "name": "adam",
            "lr": 0.0001
        },
        "print-every": 100,
        "early-stopping": 7000,
        "warmup": 5000,
        "output-dir": "outputs/",
        "seed": 42
    }
}