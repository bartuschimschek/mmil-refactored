{
    "dataset": {
        "datasets": [
            {
                "name": "control",
                "root-dir": "data/haber-2017/",
                "h5ad-filename": "ctrl.h5ad"
            },
            {
                "name": "hpoly3",
                "root-dir": "data/haber-2017/",
                "h5ad-filename": "hpoly3.h5ad"
            },
            {
                "name": "hpoly10",
                "root-dir": "data/haber-2017/",
                "h5ad-filename": "hpoly10.h5ad"
            },
            {
                "name": "salmonella",
                "root-dir": "data/haber-2017/",
                "h5ad-filename": "salm.h5ad"
            }
        ],
        "batch_size": 256,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscvae",
        "params": {
            "paired": false,
            "recon_coef": 1,
            "kl_coef": 1e-5,
            "cross_coef": 0,
            "integ_coef": 0,
            "cycle_coef": 0,
            "x_dims": [2975, 2975, 2975, 2975],
            "z_dim": 20,
            "h_dim": 256,
            "hiddens": [],
            "shared_hiddens": [128],
            "dropout": 0.2
        }
    },
    "train": {
        "n-epochs": 100000,
        "optimizer": {
            "name": "adam",
            "lr": 0.0001
        },
        "print-every": 100,
        "early-stopping": 5000,
        "warmup": 4000,
        "kl-annealing": 3000,
        "output-dir": "outputs/",
        "seed": 42
    }
}