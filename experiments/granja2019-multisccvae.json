{
    "dataset": {
        "datasets": [
            {
                "name": "scrna",
                "h5ad-path": "data/granja-2019/scRNA/expressions-ctrl.h5ad",
                "pair-group": null
            },
            {
                "name": "scatac",
                "h5ad-path": "data/granja-2019/scATAC/peaks/peaks-binary-ctrl.h5ad",
                "pair-group": null
            }
        ],
        "batch_size": 128,
        "seed": 42,
        "val_split": 0.2,
        "pair-split": 0
    },
    "model": {
        "name": "multisccvae",
        "params": {
            "pair_groups": [null, null],
            "recon_coef": 1,
            "kl_coef": 1e-5,
            "cross_coef": 0,
            "integ_coef": 1,
            "cycle_coef": 0,
            "x_dims": [3172, 10003],
            "z_dim": 20,
            "h_dim": 256,
            "hiddens": [300],
            "shared_hiddens": [128, 64],
            "dropout": 0.2
        }
    },
    "train": {
        "n-epochs": 100000,
        "optimizer": {
            "name": "adam",
            "lr": 0.0001
        },
        "print-every": 100,
        "early-stopping": 20000,
        "warmup": 10000,
        "kl-annealing": 5000,
        "output-dir": "outputs/",
        "seed": 42
    }
}