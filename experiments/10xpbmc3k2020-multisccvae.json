{
    "dataset": {
        "datasets": [
            {
                "name": "scrna",
                "h5ad-path": "data/10xpbmc3k-2020/expressions.h5ad",
                "pair-group": 0
            },
            {
                "name": "scatac",
                "h5ad-path": "data/10xpbmc3k-2020/peaks.h5ad",
                "pair-group": 0
            }
        ],
        "batch_size": 64,
        "seed": 42,
        "val_split": 0.2,
        "pair-split": 1
    },
    "model": {
        "name": "multisccvae",
        "params": {
            "pair_groups": [0, 0],
            "recon_coef": 1,
            "kl_coef": 1e-4,
            "cross_coef": 0,
            "integ_coef": 1,
            "cycle_coef": 0,
            "adver_coef": 0,
            "x_dims": [4000, 40130],
            "z_dim": 20,
            "h_dim": 256,
            "hiddens": [300],
            "shared_hiddens": [128, 64],
            "adver_hiddens": [10],
            "dropout": 0.2
        }
    },
    "train": {
        "n-epochs": 150000,
        "optimizer": {
            "name": "adam",
            "lr": 0.0001
        },
        "print-every": 200,
        "ae-optimize-every": 1,
        "early-stopping": 50000,
        "warmup": 10000,
        "kl-annealing": 10000,
        "track-metric": "integ",
        "output-dir": "outputs/",
        "seed": 42
    }
}