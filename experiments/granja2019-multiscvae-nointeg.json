{
    "dataset": {
        "datasets": [
            {
                "name": "scrna",
                "root-dir": "data/granja-2019/scRNA/",
                "h5ad-filename": "expressions-ctrl.h5ad"
            },
            {
                "name": "scatac",
                "root-dir": "data/granja-2019/scATAC/peaks/",
                "h5ad-filename": "peaks-binary-ctrl.h5ad"
            }
        ],
        "batch_size": 256,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscvae",
        "params": {
            "paired": false,
            "recon_coef": 1,
            "kl_coef": 1e-5,
            "cross_coef": 0,
            "integ_coef": 0,
            "cycle_coef": 0,
            "x_dims": [3172, 10003],
            "z_dim": 20,
            "h_dim": 256,
            "hiddens": [300],
            "shared_hiddens": [128, 64],
            "dropout": 0.2
        }
    },
    "train": {
        "n-epochs": 50000,
        "optimizer": {
            "name": "adam",
            "lr": 0.0001
        },
        "print-every": 100,
        "early-stopping": 10000,
        "warmup": 5000,
        "kl-annealing": 5000,
        "output-dir": "outputs/",
        "seed": 42
    }
}