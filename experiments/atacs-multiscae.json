{
    "dataset": {
        "datasets": [
            {
                "name": "10xgenomics",
                "root-dir": "data/atac/",
                "h5ad-filename": "10xgenomics.h5ad"
            },
            {
                "name": "fang2019",
                "root-dir": "data/atac/",
                "h5ad-filename": "fang2019.h5ad"
            },
            {
                "name": "GSE111586",
                "root-dir": "data/atac/",
                "h5ad-filename": "gse111586.h5ad"
            }
        ],
        "batch_size": 128,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscae",
        "params": {
            "paired": false,
            "recon_coef": 1,
            "cross_coef": 0,
            "integ_coef": 1,
            "cycle_coef": 0,
            "integ_shared_layers": false,
            "x_dims": [20003, 20003, 20003],
            "z_dim": 20,
            "h_dim": 256,
            "hiddens": [300],
            "shared_hiddens": [128, 64],
            "dropout": 0.2
        }
    },
    "train": {
        "n-epochs": 50000,
        "optimizer": {
            "name": "adam",
            "lr": 0.001
        },
        "print-every": 100,
        "early-stopping": 7000,
        "warmup": 5000,
        "output-dir": "outputs/",
        "seed": 42
    }
}