{
    "dataset": {
        "datasets": [
            {
                "name": "control",
                "root-dir": "data/kang-2018/",
                "h5ad-filename": "ctrl.h5ad"
            },
            {
                "name": "stimulated",
                "root-dir": "data/kang-2018/",
                "h5ad-filename": "stim.h5ad"
            }
        ],
        "batch_size": 256,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscvae",
        "params": {
            "paired": false,
            "recon_coef": 1,
            "kl_coef": 1e-6,
            "cross_coef": 0,
            "integ_coef": 0,
            "cycle_coef": 0,
            "x_dims": [3235, 3235],
            "z_dim": 20,
            "h_dim": 256,
            "hiddens": [],
            "shared_hiddens": [128, 64, 32],
            "dropout": 0.2
        }
    },
    "train": {
        "n-epochs": 50000,
        "optimizer": {
            "name": "adam",
            "lr": 0.001
        },
        "print-every": 100,
        "early-stopping": 10000,
        "warmup": 3000,
        "kl-annealing": 3000,
        "output-dir": "outputs/",
        "seed": 42
    }
}