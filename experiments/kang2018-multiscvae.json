{
    "dataset": {
        "datasets": [
            {
                "name": "kang2018-control",
                "root-dir": "data/kang-2018/",
                "h5ad-filename": "ctrl.h5ad"
            },
            {
                "name": "kang2018-stimulated",
                "root-dir": "data/kang-2018/",
                "h5ad-filename": "stim.h5ad"
            }
        ],
        "batch_size": 64,
        "seed": 42,
        "val_split": 0.2
    },
    "model": {
        "name": "multiscvae",
        "params": {
            "paired": false,
            "recon_coef": 1,
            "kl_coef": 1e-4,
            "cross_coef": 0,
            "integ_coef": 1,
            "x_dims": [3235, 3235],
            "z_dim": 20,
            "h_dim": 256,
            "hiddens": [],
            "shared_hiddens": [128, 64, 32],
            "dropout": 0.2
        }
    },
    "train": {
        "n-epochs": 1000,
        "optimizer": {
            "name": "adam",
            "lr": 0.001
        },
        "early-stopping": 200,
        "kl-annealing": {
            "start": 40,
            "period": 40
        },
        "output-dir": "outputs/",
        "seed": 42
    }
}